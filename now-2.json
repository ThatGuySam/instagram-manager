{
    "name": "instagram-manager",
    "version": 2,
    "builds": [
        { "src": "server.js", "use": "@now/node" }
    ],
    "routes": [
        { "src": "/(.*)", "dest": "server.js" },
        { "src": "/posts/(?<id>[^/]+)$", "dest": "/posts?id=$id" }
    ],
    "alias": [
      "instagram-manager.now.sh"
    ]
}