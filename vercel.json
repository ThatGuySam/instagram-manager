{
    "version": 2,
    "env": {
        "USERNAME": "@igm-username",
        "PASSWORD": "@igm-password",
        "SCREENSHOT_ENDPOINT": "@screenshot-endpoint"
    },
    "routes": [
        { "src": "/post-mockup/(?<id>[^/]+)$", "dest": "/post-mockup?id=$id" },
        { "src": "/post-preview/(?<id>[^/]+)$", "dest": "/post-preview?id=$id" }
    ],
    "alias": [
      "instagram-manager.now.sh"
    ]
}